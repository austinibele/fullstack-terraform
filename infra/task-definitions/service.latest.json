{
    "family": "task-definition-node",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::349514606126:role/Policy-Ecs-Execution-Role",
    "containerDefinitions": [
      {
        "name": "frontend",
        "image": "nginx:latest",
        "memoryReservation": 1024,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000
          }
        ],
        "environment": [
          {
            "name": "PORT",
            "value": "3000"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/aws/ecs/my-visa-usa-prod",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        },
        "secrets": []
      }
    ]
  }